@using EarringsBusinessLogic.Authentication.Contracts;
@using EarringsBusinessLogic.Authentication;
<div>
    @{
        IAuthenticator module = new AuthenticationModule();
        if(module.Authenticate())
        {
            <ul class="nav navbar-nav navbar-right">
                <li>@Html.ActionLink("Profile", "Login", "User")</li>
                <li>@Html.ActionLink("Logout", "Login", "User")</li>
            </ul>
        }
        else
        {

            <ul class="nav navbar-nav navbar-right">
                <li>@Html.ActionLink("Login", "Login", "User")</li>
                <li>@Html.ActionLink("Register", "Register", "User")</li>
            </ul>
        }
    }
</div>
